{
    "version": 2,
    "builds": [
      {
        "src": "src/web/app.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      { "src": "/Background.png", "dest": "/Background.png" },
      { "src": "/favicon.ico", "dest": "/favicon.ico" },
      { "src": "/(.*)", "dest": "src/web/app.py" }
    ]
  }